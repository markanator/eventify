		"eslint-plugin-prettier": "^5.2.3",
		"eslint-plugin-react": "^7.37.4",
		"eslint-plugin-react-hooks": "^5.1.0",
		"prettier": "^3.5.2",
		"typescript": "^5.7.3",
		"vite": "^6.1.1",
		"vite-plugin-mkcert": "^1.17.6",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.0.6"
