		"@emotion/styled": "^11.14.0",
		"@heroicons/react": "^2.2.0",
		"@loadable/component": "^5.16.4",
		"@microsoft/signalr": "^8.0.7",
		"axios": "^1.7.9",
		"dayjs": "^1.11.13",
		"formik": "^2.4.6",
		"framer-motion": "^12.4.7",
		"mobx": "^6.13.6",
		"mobx-react-lite": "^4.1.0",
		"react": "^19.0.0",
		"react-calendar": "^5.1.0",
		"react-cropper": "^2.3.3",
		"react-datepicker": "^8.1.0",
		"react-dom": "^19.0.0",
