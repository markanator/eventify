{
	"name": "client-app",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --https",
		"build": "tsc && vite build",
		"postbuild": "rimraf ../API/wwwroot && cp -r ./wwwroot ../API",
		"preview": "vite preview",
		"lint": "eslint src/**/*.{ts,tsx}",
